# --- secrets & env ---
.env
scripts/.env
.state/*
!.state/.gitkeep

# --- data outputs (raw) ---
data/raw/*
!data/raw/latest.csv
!data/raw/.gitkeep

# --- processed artifacts & vector stores ---
# Ignore processed by default, then explicitly allow the files we want to publish
data/processed/*
!data/processed/.gitkeep
!data/processed/latest_enriched.csv
!data/processed/digest_latest.txt
!data/processed/goal_nudges_latest.txt

# Publish insights (weekly & monthly digests/subjects) but keep debug out
!data/processed/insights/
!data/processed/insights/**
data/processed/insights/_debug/
data/processed/insights/_debug/**

# Vectorstore stays untracked
vectorstore/
vectorstore/**

# --- notebooks ---
**/.ipynb_checkpoints/

# --- python build artifacts ---
__pycache__/
*.pyc

# --- OS cruft ---
.DS_Store
Thumbs.db

# --- scripts runtime scratch (if any) ---
scripts/.state/
scripts/data/
